<Window x:Name="welcomePage" x:Class="data_report7.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:data_report7"
        mc:Ignorable="d"
        Title="Reporte de datos Dane" Height="700" Width="1000" ResizeMode="NoResize" FontSize="20" FontWeight="Bold" Icon="/icon.ico" Background="#FFC6BFF7">
    <Grid>
        <Label Content="¡Bienvenido a la aplicación!" HorizontalAlignment="Center" VerticalAlignment="Top" FontSize="36" FontFamily="Century Gothic" FontWeight="Bold" Margin="0,40,0,0"/>
        <Label Content="Haz click en el boton para comenzar" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,507,0,0"/>
        <Button x:Name="btnImportDataPage" Content="Empezar" HorizontalAlignment="Center" Margin="0,548,0,0" VerticalAlignment="Top" BorderBrush="Black" Background="#FFFFE04F" FontSize="25" FontWeight="Bold" Click="btnImportDataPage_Click"/>
        <Image HorizontalAlignment="Left" Height="297" Margin="325,160,0,0" VerticalAlignment="Top" Width="327" Source="/colombia.png" Stretch="Fill"/>
    </Grid>
</Window>
